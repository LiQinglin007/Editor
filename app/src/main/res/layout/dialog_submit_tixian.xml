<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:orientation="vertical"
              android:background="@color/color_white"
              android:layout_marginBottom="@dimen/px_10"
              android:layout_height="wrap_content">

    <TextView
        android:id="@+id/dialog_title"
        android:text="提示"
        android:layout_marginTop="@dimen/px_30"
        android:textColor="@color/color_ff7200"
        android:textSize="@dimen/px_36"
        android:gravity="center_horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/dialog_tv1"
        android:layout_marginTop="@dimen/px_30"
        android:layout_width="match_parent"
        android:paddingRight="@dimen/px_24"
        android:paddingLeft="@dimen/px_24"
        android:text="如您已实名认证，此次提现将于2-48小时到账，请注意查询支付宝到账情况。未实名认证的用户，冻结金额2-48小时会自动返还账户余额
"
        android:gravity="center_horizontal"
        android:layout_height="wrap_content"/>

    <TextView
        android:id="@+id/dialog_submit"
        android:layout_marginTop="@dimen/px_50"
        android:text="确定"
        android:paddingTop="@dimen/px_10"
        android:paddingBottom="@dimen/px_10"
        android:paddingLeft="@dimen/px_45"
        android:textColor="@color/color_white"
        android:paddingRight="@dimen/px_45"
        android:background="@drawable/back_sel"
        android:layout_gravity="center_horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>


</LinearLayout>
